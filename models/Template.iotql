--
--  compile to JSON with 'iotql-model Plugfest.iotql'
--
CREATE MODEL Plugfest WITH
    schema:name = "Plugfest",
    schema:description = "Plugfest"
ATTRIBUTE on WITH
    schema:name = "on",
    iot:purpose = iot-purpose:on,
    iot:type = iot:type.boolean,
    iot:write = true,
    iot:read = true,
    iot:actuator = true,
    iot:sensor = true
;
