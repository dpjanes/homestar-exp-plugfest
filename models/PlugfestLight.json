{
  "@context": {
    "@base": "file:///plugfest-light",
    "@vocab": "file:///plugfest-light#",
    "iot": "https://iotdb.org/pub/iot#",
    "schema": "http://schema.org/",
    "iot-purpose": "https://iotdb.org/pub/iot-purpose#",
    "iot:purpose": {
      "@id": "https://iotdb.org/pub/iot#purpose",
      "@type": "@id"
    },
    "iot:type": {
      "@id": "https://iotdb.org/pub/iot#type",
      "@type": "@id"
    },
    "iot-unit": "https://iotdb.org/pub/iot-unit#",
    "iot:unit": {
      "@id": "https://iotdb.org/pub/iot#unit",
      "@type": "@id"
    }
  },
  "@id": "",
  "@type": "iot:Model",
  "schema:name": "Plugfest Light",
  "schema:description": "Kinda a Pseudo Hue",
  "iot:attribute": [
    {
      "@type": "iot:Attribute",
      "@id": "#on",
      "schema:name": "on",
      "iot:purpose": "iot-purpose:on",
      "iot:type": "iot:type.boolean",
      "iot:write": true,
      "iot:read": true,
      "iot:actuator": true,
      "iot:sensor": true
    },
    {
      "@type": "iot:Attribute",
      "@id": "#brightness",
      "schema:name": "brightness",
      "iot:purpose": "iot-purpose:color.hsb.brightness",
      "iot:type": "iot:type.number",
      "iot:unit": "iot-unit:math.fraction.unit",
      "iot:vector": "hsb",
      "iot:minimum": 0,
      "iot:maximum": 1,
      "iot:write": true,
      "iot:read": true,
      "iot:actuator": true,
      "iot:sensor": true
    },
    {
      "@type": "iot:Attribute",
      "@id": "#hue",
      "schema:name": "hue",
      "iot:purpose": "iot-purpose:color.hsb.hue",
      "iot:unit": "iot-unit:math.fraction.number",
      "iot:vector": "hsb",
      "iot:minimum": 0,
      "iot:maximum": 1,
      "iot:write": true,
      "iot:read": true,
      "iot:actuator": true,
      "iot:sensor": true
    },
    {
      "@type": "iot:Attribute",
      "@id": "#saturation",
      "schema:name": "saturation",
      "iot:purpose": "iot-purpose:color.hsb.saturation",
      "iot:type": "iot:type.number",
      "iot:unit": "iot-unit:math.angle.degree",
      "iot:vector": "hsb",
      "iot:minimum": 0,
      "iot:maximum": 360,
      "iot:write": true,
      "iot:read": true,
      "iot:actuator": true,
      "iot:sensor": true
    },
    {
      "@type": "iot:Attribute",
      "@id": "#x",
      "schema:name": "x",
      "iot:purpose": "iot-purpose:color.cie.x",
      "iot:type": "iot:type.number",
      "iot:unit": "iot-unit:math.fraction.unit",
      "iot:vector": "xy",
      "iot:minimum": 0,
      "iot:maximum": 1,
      "iot:read": true,
      "iot:sensor": true,
      "iot:write": false,
      "iot:actuator": false
    },
    {
      "@type": "iot:Attribute",
      "@id": "#y",
      "schema:name": "y",
      "iot:purpose": "iot-purpose:color.cie.y",
      "iot:unit": "iot-unit:math.fraction.unit",
      "iot:type": "iot:type.number",
      "iot:vector": "xy",
      "iot:read": true,
      "iot:sensor": true,
      "iot:write": false,
      "iot:actuator": false
    },
    {
      "@type": "iot:Attribute",
      "@id": "#colorTemperature",
      "schema:name": "color temperature",
      "iot:purpose": "iot-purpose:color.temperature",
      "iot:type": "iot:type.number",
      "iot:unit": "iot-unit:temperature.si.mirek",
      "iot:read": true,
      "iot:sensor": true,
      "iot:write": false,
      "iot:actuator": false
    },
    {
      "@type": "iot:Attribute",
      "@id": "#colorMode",
      "schema:name": "color mode",
      "iot:purpose": "iot-purpose:value",
      "iot:type": "iot:type.string",
      "iot:enumeration": [
        "hs",
        "ct",
        "xy"
      ],
      "iot:sensor": true,
      "iot:read": true,
      "iot:write": false,
      "iot:actuator": false
    }
  ]
}
